<html>

<head>
    <script src="http://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.js"></script>
    <link rel="stylesheet" type="text/css" href="resources/youtube.css">
    <script type="text/javascript">
        const RADIUS = 50;
    </script>
</head>

<body>

    <div id="header">
        <div id="toggle"></div>
        <div id="youtube"></div>
        <div id="search">
            <div id="search-bar"></div>
            <div id="search-icon"></div>
        </div>
        <div id="right-block">
            <div id="profile"></div>
            <div id="notify"></div>
            <div id="nine-grid"></div>
            <div id="upload"></div>
        </div>
    </div>

    <div id="side-bar">
        <div id="home"></div>
        <div id="trending"></div>
        <div id="subscriptions"></div>
        <div id="library">
            <div id="library1"></div>
            <div id="library2"></div>
            <div id="library3"></div>
            <div id="library4"></div>
            <div id="library5"></div>
        </div>
        <div id="channels">
            <div id="channel01"></div>
            <div id="channel02"></div>
            <div id="channel03"></div>
            <div id="channel04"></div>
            <div id="channel05"></div>
            <div id="channel06"></div>
            <div id="channel07"></div>
            <div id="channel08"></div>
            <div id="channel09"></div>
            <div id="channel10"></div>
            <div id="channel11"></div>
        </div>
        <div id="browse"></div>
        <div id="settings"></div>
        <div id="help"></div>
    </div>

    <div id="main">
        <div id="video"></div>
        <div id="control-bar">
            <div id="control-icon1"></div>
            <div id="control-icon2"></div>
            <div id="control-icon3"></div>
            <div id="control-icon4"></div>
            <div id="control-icon5"></div>
            <div id="control-icon6"></div>
        </div>
        <div id="reaction">
            <div id="like"></div>
            <div id="dislike"></div>
            <div id="share"></div>
            <div id="playlist"></div>
            <div id="options"></div>
        </div>
        <div id="sigchi"></div>
        <div id="subscribe"></div>
        <div id="show-more"></div>
        <div id="sort-by"></div>
    </div>

    <div id="up-next">
        <div id="auto"></div>
        <div id="video01"></div>
        <div id="video02"></div>
        <div id="video03"></div>
        <div id="video04"></div>
        <div id="video05"></div>
        <div id="video06"></div>
        <div id="video07"></div>
        <div id="video08"></div>
        <div id="video09"></div>
        <div id="video10"></div>
    </div>


    <% if(process.argv[4] === 'mobile'){ %>
        <div id="MobileContainer"></div>
    <% } %>

    <div id="canvas_container">
        <canvas id="canvas" width="1400" height="1000"></canvas>
    </div>

    <div id="eye_tracker" class="<%=EYETRACKER%>"></div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="resources/index.js"></script>

</body>

</html>